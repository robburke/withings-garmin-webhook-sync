# Withings API Credentials
# Get these from: https://developer.withings.com/dashboard
WITHINGS_CLIENT_ID=your_client_id_here
WITHINGS_CLIENT_SECRET=your_client_secret_here
WITHINGS_CALLBACK_URI=http://localhost:5000/callback

# Withings Refresh Token
# Leave empty initially - will be populated by setup.py
WITHINGS_REFRESH_TOKEN=

# Garmin Connect Credentials
GARMIN_EMAIL=your_garmin_email@example.com
GARMIN_PASSWORD=your_garmin_password

# Application Settings
PORT=5000
LOG_LEVEL=INFO
